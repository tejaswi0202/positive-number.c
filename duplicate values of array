#include <stdio.h>
void main()
{
    int a[10],count=0,i,j,n;
    printf("enter size:");
    scanf("%d",&n);
    printf("enter the elements:");

    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
        for(i=0;i<n;i++)
{
        for(j=i+1;j<n;j++)
        {
            if(a[i]==a[j])
            {
                count++;
                break;
            }
            }
}
        printf("the number of duplicate values is%d",count);
}
        
