#include <stdio.h>
void main()
{
    int a[10],b[10],i,n,j=0,k=0,c[10];
    printf("enter size:");
    scanf("%d",&n);
    printf("enter the elements:");

    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
      if(a[i]%2==0)
      {
          b[j]=a[i];
          j++;
      }
     else
     {
         c[k]=a[i];
         k++;
     }
    }
     for(i=0;i<j;i++)
     {
         printf("%d \n",b[i]);
     }
     for(i=0;i<k;i++)
    {
        printf("%d\n",c[i]);
    }
}
